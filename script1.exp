#!/usr/bin/expect -f
spawn ssh -q subham@10.10.100.101
expect "password: "
send "subham\r"
expect "$ "
send "ps -ef|grep smb\r"
expect "$ "
send "su - rcxdev\r"
expect "Password:"
send "rcxdev\r"
expect "$ "
send "sudo su -\r"
expect "# "
send "useradd mahi\r"
expect "# "
send "cat /etc/passwd\r"
expect "# "
send "passwd mahi\r"
expect "Enter new UNIX password:"
send "mahi\r"
expect "Retype new UNIX password:"
send "mahi\r"
expect "# "
send "exit\r"
